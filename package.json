{
  "name": "vite-plugin-cesium-assets",
  "version": "1.0.1",
  "description": "A Vite plugin to automatically copy and serve Cesium library assets, enabling seamless Cesium integration in Vite projects.",
  "main": "./lib/index.cjs",
  "module": "./lib/index.mjs",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/index.mjs",
      "require": "./lib/index.cjs"
    }
  },
  "keywords": [
    "vite",
    "vite-plugin",
    "cesium"
  ],
  "author": "4Nameless4 <nameless9744@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "rollup --config rollup.config.ts --configPlugin typescript",
    "test": "pnpm run build && cd test && pnpm install && pnpm run dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/4Nameless4/vite-plugin-cesium-assets.git"
  },
  "bugs": {
    "url": "https://github.com/4Nameless4/vite-plugin-cesium-assets/issues"
  },
  "homepage": "https://github.com/4Nameless4/vite-plugin-cesium-assets#readme",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.3.0",
    "@types/node": "^20.10.3",
    "@types/serve-static": "^2.2.0",
    "rollup": "^4.53.3",
    "rollup-plugin-dts": "^6.3.0",
    "rollup-plugin-license": "^3.6.0",
    "terser": "^5.44.1",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "serve-static": "^1.14.1"
  },
  "peerDependencies": {
    "cesium": "^1.95.0",
    "vite": ">=2.7.1"
  },
  "files": [
    "lib",
    "package.json",
    "README.md",
    "README.zh-CN.md"
  ]
}
